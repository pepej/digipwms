<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_pricking_form" model="ir.ui.view">
        <field name="name">stock.picking.view.form.inherit.module.name</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button type="object" name="enviar" string="Enviar DigipWMS"  />
            </xpath>
            <xpath expr="//header" position="inside">
                <button type="object" name="recibir" string="Recibir DigipWMS"  />
            </xpath>
        </field>
    </record>
</odoo>
